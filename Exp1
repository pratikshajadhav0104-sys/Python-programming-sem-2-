"""

num1=int(input("Enter number1:"))
num2=int(input("Enter number2:"))

sum=num1+num2
print("addition=",sum)

sub=num1-num2
print("substraction=",sub)

mul=num1*num2
print("multiplication=",mul)

div=num1/num2 
print("division=",div)

mod=num1%num2
print("modulus=",mod)
